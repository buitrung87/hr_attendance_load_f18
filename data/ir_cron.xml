<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_attendance_device_pull" model="ir.cron">
            <field name="name">Pull Attendance From F18 Devices</field>
            <field name="model_id" ref="model_attendance_device"/>
            <field name="state">code</field>
            <field name="code">model.search([('active', '=', True), ('auto_pull', '=', True)]).action_pull_attendance()</field>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="active">True</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="priority">5</field>
        </record>
    </data>
</odoo>